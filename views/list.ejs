<%- include("header") -%>



    <div class="box" id="heading">
        <h1>
            <%= kindofDay %>
        </h1>


    </div>

    <div class="box">
        <% for(var i=0; i<newListitems.length; i++){ %>
            <div class="item">
                <input type="checkbox">
                <p>

                    <%=newListitems[i]%>
                </p>

            </div>





            <%} %>


                <form action="/" method="post" class="item">
                    <input type="text" name="newItem" placeholder="add new..." autocomplete="off">

                    <button type="submit" name="List" value="<%= kindofDay %>">+</button>


                </form>


    </div>
    <%- include("footer") -%>